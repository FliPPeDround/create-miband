{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "globals": {
        "$": true,
        "hmUI": true,
        "__$$hmAppManager$$__": true,
        "DeviceRuntimeCore": true,
        "Logger": true
    },
    "rules": {
        "no-new":"off"
    }
}
